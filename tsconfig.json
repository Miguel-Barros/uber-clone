{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "screens/*": [
        "src/screens/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "contexts/*": [
        "src/contexts/*"
      ],
    }
  },
}